                                    form(name='settingsForm',action='/profile', method='post',ng-submit='submitForm()', novalidate='')
                                        .form-group(ng-class="{ 'has-error' : settingsForm.firstname.$invalid && (settingsForm.firstname.$touched || submitted)}")
                                            label.col-sm-2.control-label Firstname
                                            .col-sm-10.controls
                                                .row
                                                    .col-xs-12
                                                        input.form-control(type='text', name='firstname', ng-model='setting.firstname', placeholder='Your Firstname', ng-minlength='2', ng-maxlength='15', ng-required='true')
                                                        .help-block(ng-messages='settingsForm.firstname.$error', ng-if='settingsForm.firstname.$touched')
                                                            p(ng-message='minlength') Your firstname is too short.
                                                            p(ng-message='maxlength') Your firstname is too long.
                                                            p(ng-message='required') Your firstname is required.

                                        .form-group(ng-class="{ 'has-error' : settingsForm.lastname.$invalid && (settingsForm.lastname.$touched || submitted)}")
                                            label.col-sm-2.control-label Lastname
                                            .col-sm-10.controls
                                                .row
