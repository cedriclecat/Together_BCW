                    #accordion.panel-group(ng-controller='EventController', role='tablist', aria-multiselectable='true')
                        div(ng-repeat='something in data')
                            .panel.panel-default
                                .panel-heading(role='tab', id='headingOne')
                                    h4.panel-title
                                        a.collapsed(role='button', data-toggle='collapse', data-parent='#accordion1', href='#collapseOne', aria-expanded='false', aria-controls='collapseOne')
                                            img(ng-src='{{something.pictureUrl}}')
                                            |  {{something.name}}
                                .panel-collapse.collapse(id='collapseOne', role='tabpanel', aria-labelledby='headingOne')
                                    .panel-body
                                        iframe(src='https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2501.703217746505!2d4.139664715962859!3d51.169260643190526!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47c38ebd60f3c64f%3A0xd95f2b6450890892!2sDe+Casino!5e0!3m2!1snl!2sbe!4v1446294324853', width='400', height='300', frameborder='0', style='border:0', allowfullscreen='')
                                        p.info Praktische Informatie
                                        p
                                            i.fa.fa-calendar
                                            | {{something.date}}
                                        p
                                            i.fa.fa-clock-o
                                            | {{something.time}}
                                        p
                                            i.fa.fa-eur
                                            | {{something.price}}
                                        p
                                            i.fa.fa-map-marker
                                            | {{something.location}}
                                        p {{something.description}}
